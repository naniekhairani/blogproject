<h1>New photo</h1>

<% form_for(@photo) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :filename%><br/>
<%= f.text_field :filename%>  
</p>
<p>
  <%= f.label :description %><br />
    <%= f.text_area :description %>
        
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', photos_path %>
